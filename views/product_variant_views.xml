<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_product_variant_form_inherit_purchase_commission" model="ir.ui.view">
        <field name="name">product.variant.form.inherit.purchase.commission</field>
        <field name="model">product.product</field>
        <field name="inherit_id" ref="product.product_normal_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='purchase_ok']" position="after">
                <field name="remote_product_id"/>
            </xpath>
        </field>
    </record>
</odoo>